<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" 
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="H2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="org.h2.Driver"
			connectionURL="jdbc:h2:file:${user.home}/itms/data/itms" userId="sa"
			password="">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="com.zjcds.itms.domain.entity"
			targetProject="target">
			<property name="enableSubPackages" value="false" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.zjcds.dp.cdm.dao.mybatis"
			targetProject="target">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		
		<table tableName="t_sys_log" domainObjectName="SysLog"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false" alias="t">
		</table>

		<table tableName="t_ds" domainObjectName="Ds"
			   enableCountByExample="false" enableUpdateByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   selectByExampleQueryId="false" alias="t">
		</table>

		<table tableName="t_ds_param" domainObjectName="DsParam"
			   enableCountByExample="false" enableUpdateByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   selectByExampleQueryId="false" alias="t">
		</table>

		<table tableName="t_ds_type_property_template" domainObjectName="DsTypePropertyTemplate"
			   enableCountByExample="false" enableUpdateByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   selectByExampleQueryId="false" alias="t">
		</table>

	</context>
</generatorConfiguration> 